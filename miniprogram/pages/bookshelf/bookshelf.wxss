.head{
  display: flex;
  justify-content: flex-end;
  font-size: 28rpx;
  padding: 10rpx 20rpx;
  position: fixed;
  z-index: 999;
  top: 0;
  left: 0;
  width: 100%;
  height: 60rpx;
  box-sizing: border-box;
  border-bottom: 1px solid #eee;
  background-color: #fff;
}

.container{
  margin-top: 60rpx;
  padding: 20rpx;
  display: grid;
  grid-template-columns:  repeat(4, 1fr);
  grid-row-gap: 30rpx;
  justify-items:center;
  
}
